{
  "name": "minvws/nl-rdo-zammad-api-export",
  "description": "Zammad ticket export",
  "license": "EUPL",
  "keywords": [
    "zammad"
  ],
  "autoload": {
    "psr-4": {
      "Minvws\\Zammad\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MinVWS\\Zammad\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Joshua Thijssen",
      "email": "jthijssen@noxlogic.nl"
    },
    {
      "name": "Rick Lambrechts",
      "email": "rick@rl-webdiensten.nl"
    }
  ],
  "config": {
      "platform": {
         "php": "8.0.2"
      },
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  "require": {
    "php": "^8.0.2",
    "zammad/zammad-api-client-php": "^2.1.0",
    "symfony/console": "^6.0",
    "symfony/dotenv": "^6.0",
    "twig/twig": "^3.0",
    "indiehd/filename-sanitizer": "^0.1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^5.1",
    "phpstan/phpstan": "^2.0",
    "squizlabs/php_codesniffer": "^4.0",
    "mockery/mockery": "^1.4.3"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs",
      "vendor/bin/phpunit",
      "vendor/bin/phpstan analyse",
      "vendor/bin/psalm"
    ]
  }
}
